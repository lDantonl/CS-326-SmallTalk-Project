<!DOCTYPE html>
<html>
  <head>
    <title><%= User.username %>:Followers</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= User.username %></h1>
    <p>Here are your Followers</p>
    <% User.followers.forEach(function(follower){ %>
    	<li><%= follower %></li>
    <% });%>
  </body>
</html>
